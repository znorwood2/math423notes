                  \draw [postaction={decorate}] (E) to (NE1);
                  \draw [postaction={decorate}] (NW1) to (NE1) ;
                  \draw [postaction={decorate}] (NE1) to (WE1) ;
                  \draw [postaction={decorate}] (WE1) to (NW1) ;
                \end {scope}
                 \end {tikzpicture}
                 \begin {tikzpicture}
                 \coordinate (N) at (0,3) ;
